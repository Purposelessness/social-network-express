{
  "main": "./src/server.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf ./build && npx tsc && npx tsc-alias",
    "build-webpack": "rimraf ./public/scripts && cross-env NODE_ENV=development DIRNAME='public' npx webpack",
    "build-gulp": "rimraf ./dist && cross-env NODE_ENV=development DIRNAME='dist' npx gulp",
    "build-gulp-public": "rimraf ./public/scripts && cross-env NODE_ENV=development DIRNAME='public' npx gulp",
    "start": "npm run build && npm run build-webpack && node build/server",
    "fast-start": "npm run build && node build/server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
